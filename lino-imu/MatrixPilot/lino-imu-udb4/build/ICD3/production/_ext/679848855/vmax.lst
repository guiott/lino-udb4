  35              	;
  36              	; _VectorMax: Vector Maximum Value (and last index with maximum value).
  37              	;
  38              	; Operation:
  39              	;	maxVal = max {srcV[n], n in {0, 1,...numElems-1} }
  40              	;   residually,
  41              	;	if srcV[i] = srcV[j] = maxVal, and i < j, then *(maxIndex) = j
  42              	;
  43              	; Input:
  44              	;	w0 = number elements in vector(s) (numElems)
  45              	;	w1 = ptr to source vector (srcV)
  46              	;	w2 = ptr to index of maximum value (&maxIndex)
  47              	; Return:
  48              	;	w0 = maximum value (maxVal)
  49              	;
  50              	; System resources usage:
  51              	;	{w0..w5}	used, not restored
  52              	;
  53              	; DO and REPEAT instruction usage.
  54              	;	no DO intructions
  55              	;	no REPEAT intructions
  56              	;
  57              	; Program words (24-bit instructions):
  58              	;	13
MPLAB ASM30 Listing:  ../../libVectorMatrix/vmax.s 			page 2


  59              	;
  60              	; Cycles (including C-function call and return overheads):
  61              	;	14			(if numElems == 1)
  62              	;	20 + 8*(numElems-2)	(if srcV[n] <= srcV[n+1], 0 <= n < numElems-1)
  63              	;	19 + 7*(numElems-2)	(if srcV[n] >  srcV[n+1], 0 <= n < numElems-1)
  64              	;............................................................................
  65              	
  66              		.global	_VectorMax	; export
  67              	_VectorMax:
  68              	
  69              	;............................................................................
  70              	
  71 000000  B1 02 78 		mov	[w1++],w5			; w5 = srcV[0] (current maxVal)
  72 000002  00 09 EB 		clr	[w2]				; *w2 = current max value index
  73 000004  00 02 E9 		dec	w0,w4				; w4 = numElems-1
  74 000006  00 00 34 		bra	le,_endMax			; no more elements...
  75              	_compare:
  76 000008  B1 01 78 		mov	[w1++],w3			; w3 = srcV[n+1]
  77 00000a  03 28 E1 		cp	w5,w3				; srcV[n] < srcV[n+1] ?
  78 00000c  00 00 3C 		bra	gt,_noUpdate			; yes => no update
  79              	_doUpdate:					; no  => do update
  80 00000e  83 02 78 		mov	w3,w5				; w5 = (current maxVal)
  81 000010  04 09 50 		sub	w0,w4,[w2]			; *w2 = current max value index
  82              	_noUpdate:
  83 000012  04 02 E9 		dec	w4,w4				; w4 = numElems-n
  84 000014  00 00 3C 		bra	gt,_compare			; no more elements...
  85              	_endMax:
  86 000016  05 00 78 		mov	w5,w0				; restore return value
  87              	
  88              	;............................................................................
  89              	
  90 000018  00 00 06 		return	
