  36              	;
  37              	; _VectorCopy: initialization to zero of FIR filter delay.
  38              	;
  39              	; Operation:
  40              	;	dstV[n] = srcV[n], 0 <= n < numElems
  41              	;
  42              	; Input:
  43              	;	w0 = number elements to copy from source vector (numElems)
  44              	;	w1 = ptr to destination vector (dstV)
  45              	;	w2 = ptr to source vector (srcV)
  46              	; Return:
  47              	;	w0 = ptr to destination vector (dstV)
  48              	;
  49              	; System resources usage:
  50              	;	{w0..w3}	used, not restored
  51              	;
  52              	; DO and REPEAT instruction usage.
  53              	;	no DO instructions
  54              	;	1 level REPEAT intruction
  55              	;
  56              	; Program words (24-bit instructions):
  57              	;	6
  58              	;
  59              	; Cycles (including C-function call and return overheads):
MPLAB ASM30 Listing:  ../../libVectorMatrix/vcopy.s 			page 2


  60              	;	12 + numElems
  61              	;............................................................................
  62              	
  63              		.global	_VectorCopy	; export
  64              	_VectorCopy:
  65              	
  66 000000  00 00 F8 		push	RCOUNT
  67              	
  68              	;............................................................................
  69              	
  70 000002  81 01 78 		mov	w1,w3				; save return value (dstV)
  71              	
  72 000004  00 00 E9 		dec	w0,w0				; w0 = numElems-1
  73 000006  00 80 09 		repeat	w0				; do (numElems-1)+1 times
  74 000008  B2 18 78 		mov	[w2++],[w1++]			; dstV[n] = srcV[n]
  75              							; w1-> dstV[n+1]
  76              							; w2-> srcV[n+1]
  77              	
  78 00000a  03 00 78 		mov	w3,w0				; restore return value
  79              	
  80              	;............................................................................
  81              	
  82 00000c  00 00 F9 		pop		RCOUNT
  83              	
  84 00000e  00 00 06 		return	
  85              	
  86              	;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
  87              	
  88              		.end
