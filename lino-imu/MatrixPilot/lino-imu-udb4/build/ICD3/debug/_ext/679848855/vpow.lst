 130              	
 131              	;	restore the 40 bit A accumulator
 132 000030  00 00 F9 		pop		ACCAU
 133 000032  00 00 F9 		pop		ACCAH
 134 000034  00 00 F9 		pop		ACCAL
 135              	
 136 000036  00 00 06 		return	
